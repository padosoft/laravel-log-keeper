{
    "name": "mathiasgrimm/laravel-log-keeper",
    "description": "Laravel Log Keeper helps rotating you logs while storing them anywhere you want with custom local/remote retention policies",
    "keywords": ["laravel", "log", "s3", "retention", "dropbox", "ftp", "sftp", "rotation", "disk", "drive", "remote"],
    "authors": [
        {
            "name": "Mathias Grimm",
            "email": "mathiasgrimm@gmail.com"
        }
    ],
    "require": {
        "padosoft/support": "^1.16|^2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2|^9.0",
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0 || ^6.0",
        "mockery/mockery": "^1.2"
    },

    "autoload": {
        "psr-4": {
            "MathiasGrimm\\LaravelLogKeeper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MathiasGrimm\\LaravelLogKeeper\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MathiasGrimm\\LaravelLogKeeper\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}
