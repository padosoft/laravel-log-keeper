{
  "name": "padosoft/laravel-log-keeper",
  "description": "Forked mathiasgrimm/laravel-log-keeper Laravel Log Keeper helps rotating you logs while storing them anywhere you want with custom local/remote retention policies",
  "keywords": [
    "laravel",
    "log",
    "s3",
    "retention",
    "dropbox",
    "ftp",
    "sftp",
    "rotation",
    "disk",
    "drive",
    "remote"
  ],
  "authors": [
    {
      "name": "Mathias Grimm",
      "email": "mathiasgrimm@gmail.com"
    },
    {
      "name": "Leonardo Padovani",
      "email": "helpdesk@padosoft.com"
    }
  ],
  "require": {
    "padosoft/support": "^1.16|^2.0.0|^3.0.4"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^8.2|^9.0|^10.0",
    "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
    "mockery/mockery": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "MathiasGrimm\\LaravelLogKeeper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MathiasGrimm\\LaravelLogKeeper\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MathiasGrimm\\LaravelLogKeeper\\Providers\\LaravelServiceProvider"
      ]
    }
  }
}
